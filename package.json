    "babel-preset-react": "6.22.0",
    "babel-runtime": "^6.22.0",
    "css-loader": "0.26.1",
    "eslint": "^3.17.1",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.26.0",
