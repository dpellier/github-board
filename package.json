  "license": "MIT",
  "homepage": "https://github.com/dpellier/github-board",
  "dependencies": {
    "auth0-lock": "^10.11.0",
    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "jwt-decode": "^2.1.0",
    "moment": "^2.17.1",
    "nanoajax": "^0.4.3",
    "node-sass": "^4.3.0",
    "normalize.css": "^5.0.0",
    "q": "^1.4.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "^3.0.2",
    "reflux": "^5.0.0",
    "request-promise": "^4.1.1",
